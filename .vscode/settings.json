{
  "files.associations": {
    "**/.azurepipelines/*.yml": "azure-pipelines",
    "**LICENSE*.jinja": "jinja",
    "**.all-contributorsrc*.jinja": "jinja-json",
    "**.json*.jinja": "jinja-json",
    "**.md*.jinja*": "jinja-md",
    "copier.yml": "jinja-yaml",
    "**.y*ml*.jinja": "jinja-yaml",
    "**.json[{]% endif %[}]": "json",
    "**.md[{]% endif %[}]": "markdown",
    "**.y*ml[{]% endif %[}]": "yaml",
    "**.py[{]% endif %[}]": "python",
    "**.toml[{]% endif %[}]": "toml",
    "**.json5": "json"
  },
  "ruff.configuration": "${workspaceFolder}/.config/ruff.toml",
  "typos.config": ".config/typos.toml",
  "yamlLintFix.configFile": ".config/.yamllint.yaml"
}
